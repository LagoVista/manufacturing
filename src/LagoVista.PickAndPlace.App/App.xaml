<Application x:Class="LagoVista.PickAndPlace.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LagoVista.PickAndPlace.App"
             StartupUri="Splash.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Converters.xaml"/>
                <ResourceDictionary Source="/Resources/DefaultTheme.xaml"/>
                <ResourceDictionary Source="/Resources/Colors.Default.xaml"/>
                <ResourceDictionary Source="/Resources/Styles.xaml"/>
                <ResourceDictionary Source="/Resources/TabView.ControlTemplate.xaml"/>
                <ResourceDictionary Source="/Resources/ComboBox.ControlTemplate.xaml"/>
                <ResourceDictionary Source="/Resources/Button.ControlTemplate.xaml"/>
                <ResourceDictionary Source="/Resources/ListView.ControlTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
